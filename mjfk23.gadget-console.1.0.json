{
  "manifests": {
    "mjfk23/gadget-console": {
      "manifest": {
        "copy-from-recipe": {
          "config/": "%CONFIG_DIR%/"
        }
      },
      "files": {
        "config/packages/gadget_console.yaml": {
          "contents": [
            "services:",
            "  _defaults:",
            "    autowire: true",
            "    autoconfigure: true",
            "",
            "  Gadget\\Console\\Shell\\ProcessShellEnv:",
            "    arguments:",
            "      $workDir: '%kernel.project_dir%'",
            ""
          ],
          "executable": false
        }
      },
      "ref": ""
    }
  }
}
