{
  "manifests": {
    "mjfk23/gadget-oracle": {
      "manifest": {
        "copy-from-recipe": {
          "config/": "%CONFIG_DIR%/"
        }
      },
      "files": {
        "config/packages/gadget_oracle.yaml": {
          "contents": [
            "services:",
            "  _defaults:",
            "    autowire: true",
            "    autoconfigure: true",
            "",
            "  Gadget\\Oracle\\Command\\Input:",
            "    arguments:",
            "      $username: '%env(default::ORACLE_USER)%'",
            "      $password: '%env(default::ORACLE_PASS)%'",
            "      $connectIdentifier: '%env(default::ORACLE_CONN)%'",
            ""
          ],
          "executable": false
        }
      },
      "ref": ""
    }
  }
}
